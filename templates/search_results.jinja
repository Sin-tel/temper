<div class="search-results">
{% if res is defined %}
{% if "error" in res %}
    {{ res["error"] }}
{% else %}
<table>
    {% for key, value in res.items() %}
        <tr>
            <th> {{ key }} </th>
            {% if value is not string and value is iterable%}
                <td> {{ value[0] }} </td>
                <td style="text-align: right;"> {{ value[1] }} </td>
                <td style="text-align: right;"> {{ value[2] }} </td>
            {% else %}
                <td colspan= 3> {{ value|safe  }} </td>
            {% endif %}
        </tr>
    {% endfor %}
</table>
{% endif %}
{% endif %}
</div>
